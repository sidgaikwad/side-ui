{
  "name": "siddcn",
  "version": "0.1.0",
  "description": "Terminal UI component library with SSH access",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "bin": {
    "siddcn": "./dist/cli.js",
    "siddcn-server": "./dist/server.js"
  },
  "scripts": {
    "dev": "node --no-warnings --experimental-loader tsx/esm src/cli.tsx",
    "dev:server": "node --no-warnings --experimental-loader tsx/esm src/server.ts",
    "start": "node dist/cli.js",
    "start:server": "node dist/server.js",
    "build": "tsup",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx"
  },
  "keywords": [
    "tui",
    "terminal",
    "cli",
    "ink",
    "ssh",
    "components",
    "ui"
  ],
  "files": [
    "dist"
  ],
  "dependencies": {
    "ink": "^4.4.1",
    "react": "^18.3.1",
    "ssh2": "^1.15.0",
    "chalk": "^4.1.2",
    "gradient-string": "^2.0.2",
    "cli-spinners": "^2.9.2",
    "figures": "^3.2.0",
    "ink-text-input": "^5.0.1",
    "ink-select-input": "^5.0.0",
    "ink-spinner": "^5.0.0",
    "nanoid": "^3.3.7"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/react": "^18.3.0",
    "@types/ssh2": "^1.15.0",
    "@types/gradient-string": "^1.1.5",
    "tsup": "^8.0.0",
    "tsx": "^4.7.0",
    "typescript": "^5.5.0",
    "eslint": "^8.57.0"
  },
  "peerDependencies": {
    "react": ">=18.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
